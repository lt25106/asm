{
  "scopeName": "source.asm",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "patterns": [
    {
      "name": "keyword",
      "match": "(j(mp|l|g|e)|div|add|call|ret|((inc|push|mov|sub|lea|cmp|xor)[qlwb]))"
    },
    {
      "name": "variable.readonly",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "variable.readonly",
      "match": "%([abcd][lh]|[re]?[abcd]x|r[bsi]p|r(8|9|1[0-5])[dwb]?|[re]?[ds]il?)"
    },
    {
      "name": "storage",
      "match": "\\.(text|globl|macro|endm|byte|short|long|quad|string|data|seh_(endproc|proc|pushreg|stackalloc|setframe|endprologue))"
    },
    {
      "name": "comment",
      "begin": "#",
      "end": "$"
    },
    {
      "name": "string",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string",
      "begin": "'",
      "end": "'"
    }
  ]
}
